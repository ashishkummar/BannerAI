# Ignore node_modules (if you have any node_modules in backend)
node_modules/

# Ignore log files and temp files
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore build artifacts (if any)
build/
dist/

# Ignore .env files (for environment variables)
.env
.env.*.local

# Ignore VSCode workspace settings
.vscode/

# Ignore system-specific files
.DS_Store

# Ignore TypeScript build output (if you're using `tsc` for compiling)
*.tsbuildinfo

# Ignore package-lock.json (if you prefer to use yarn or other package manager)
package-lock.json

# Ignore coverage directory generated by testing tools (like Istanbul/nyc)
coverage/
*.lcov

# Ignore testing/debugging files
*.tgz
*.pid
*.seed
*.pid.lock

# Ignore any database-related files (if you have SQLite, etc.)
*.sqlite
*.db

# Ignore other potentially sensitive or unnecessary files
*.env
