# Ignore node_modules (if you have any node_modules in frontend)
node_modules/

# Ignore build output (if using build tools like Webpack, React-scripts, etc.)
build/
dist/

# Ignore log files and temp files
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore .env files (for environment variables)
.env
.env.*.local

# Ignore VSCode workspace settings
.vscode/

# Ignore system-specific files
.DS_Store

# Ignore package-lock.json (if you prefer to use yarn or other package manager)
package-lock.json

# Ignore testing/debugging files
*.tgz
*.pid
*.seed
*.pid.lock

# Ignore coverage directory generated by testing tools (like Istanbul/nyc)
coverage/
*.lcov

# Ignore TypeScript build output (if you're using `tsc` for compiling)
*.tsbuildinfo

# Ignore yarn integrity file
.yarn-integrity

# Ignore testing environment or temp files
*.test.js
*.test.tsx
*.test.ts

# Ignore any IDE-specific files
.idea/

# Ignore any build tools' intermediate files
.cache/
.parcel-cache/

# Ignore additional cache files from build tools
.next/
.nuxt/
dist/
out/

# Ignore any other temporary files
*.swp
*.swo
*.bak
